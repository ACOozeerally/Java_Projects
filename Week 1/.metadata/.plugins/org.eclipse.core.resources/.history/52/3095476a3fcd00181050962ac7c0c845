
public abstract class Vehicles {

	private String name;
	private int wheels;
	private int price;
	
	public Vehicles(String name, int wheels) {
		this.name = name;
		this.wheels = wheels;
	}

	

	public String getName() {
		return name;
		
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public int numberOfWheels(int wheels) {
		return wheels;
	}
	


	public int getPrice() {
		return price;
	}



	public void setPrice(int price) {
		this.price = price;
	}



	
	
	
	
	
}
